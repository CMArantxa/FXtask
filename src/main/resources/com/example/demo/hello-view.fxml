<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" minWidth="-Infinity" prefHeight="360.0" prefWidth="369.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label fx:id="lblusuario" prefHeight="17.0" prefWidth="272.0" text="Nombre de usuario" />
      <TextField fx:id="txtUsuario" />
      <Label prefHeight="17.0" prefWidth="274.0" text="Password" />
      <PasswordField fx:id="txtPassword" />
      <Button mnemonicParsing="false" onAction="#onLoginClick" text="Login" />
   </children>
</VBox>
